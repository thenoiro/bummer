{
  "name": "bummer",
  "version": "1.0.0",
  "description": "A small script that helps easily and safely get, set, remove, replace or check an object's properties located at any depth.",
  "main": "dist/bummer.js",
  "scripts": {
    "start": "NODE_ENV=production node run.js",
    "start:open": "NODE_ENV=production node run.js --open",
    "build": "NODE_ENV=production node build.js",
    "build:open": "NODE_ENV=production node build.js --dev-server --open",
    "build:dev": "NODE_ENV=development node build.js",
    "dev": "node build.js --dev-server",
    "dev:open": "node build.js --open --dev-server",
    "stop": "node stop.js",
    "test": "NODE_ENV=test node test.js"
  },
  "keywords": [
    "object",
    "path",
    "properties"
  ],
  "author": "Chernovol Bohdan",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.19.1",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "fkill": "^6.2.0",
    "html-loader": "^0.5.5",
    "markdown-loader": "^5.1.0",
    "open": "^7.0.0",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thenoiro/bummer.git"
  },
  "bugs": {
    "url": "https://github.com/thenoiro/bummer/issues"
  },
  "homepage": "https://github.com/thenoiro/bummer#readme"
}
