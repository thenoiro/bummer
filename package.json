{
  "name": "porter",
  "version": "1.0.0",
  "description": "Small script which allows easily and safely get, set, remove or check object's properties located at any depth.",
  "main": "dist/porter.min.js",
  "scripts": {
    "start": "NODE_ENV=production node run.js",
    "start:open": "NODE_ENV=production node run.js --open",
    "build": "NODE_ENV=production node build.js",
    "build:open": "NODE_ENV=production node build.js --dev-server --open",
    "build:dev": "NODE_ENV=development node build.js",
    "dev": "node build.js --dev-server",
    "dev:open": "node build.js --open --dev-server",
    "stop": "node stop.js",
    "test": "NODE_ENV=test node test.js"
  },
  "keywords": [
    "object",
    "path",
    "properties"
  ],
  "author": "Chernovol Bohdan",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "fkill": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "open": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thenoiro/porter.git"
  },
  "bugs": {
    "url": "https://github.com/thenoiro/porter/issues"
  },
  "homepage": "https://github.com/thenoiro/porter#readme"
}
